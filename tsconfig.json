{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "jsx": "react-native",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "esnext"
    ],
    "sourceMap": true,
    "outDir": "./dist",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "./",
    "paths": {
      "*": [
        "node_modules/*",
        "src/*",
        "styles/*",
      "context/*",
      "screens/*",
      "navigation/*",
      "assets/*",
      "components/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "app.json"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}